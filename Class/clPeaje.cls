'Public Function oResultanteControl(ByVal fDesde As String, ByVal fHasta As String) As ADODB.Recordset
'strSql1 = "SELECT a.CODIGO_CAJERO, a.IDENTIDAD_USUARIO, b.CODIGO_SUBSECTOR, b.DESCRIPCION_SUBSECTOR, SUM(a.SISTEMA) AS SISTEMA, SUM(a.LIQ) AS LIQ,"
'strSql1 = strSql1 & vbCrLf & "     SUM(a.SDOCONTEO) AS SDOCONTEO, SUM(A.TICK_ANUV) AS TICK_LIQ, SUM(A.TICK_ANUL) - SUM(A.TICK_ANUV) AS TICK_DIF, SUM(A.BMFALSOS) AS BMFALSOS,"
'strSql1 = strSql1 & vbCrLf & "     SUM(a.TOTALDESC) AS TOTALDESC, SUM(A.TOTALDISC) AS TOTALDISC FROM("
'strSql1 = strSql1 & vbCrLf & "     SELECT A.CODIGO_CAJERO, A.CODIGO_ESTACION, H.IDENTIDAD_USUARIO,"
'strSql1 = strSql1 & vbCrLf & "     TO_NUMBER(SUM(A.C01_METALICO+A.C02_METALICO+A.C03_METALICO+A.C04_METALICO+A.C05_METALICO+A.C06_METALICO+A.C07_METALICO)) AS SISTEMA,"
'strSql1 = strSql1 & vbCrLf & "     SUM(A.CONT1_MONEDA_LOCAL) AS LIQ,"
'strSql1 = strSql1 & vbCrLf & "     SUM(A.RECAUD2) AS SDOCONTEO,"
'strSql1 = strSql1 & vbCrLf & "     TO_NUMBER(F.TICK_ANUL) AS TICK_ANUL,"
'strSql1 = strSql1 & vbCrLf & "     TO_NUMBER(F.TICK_ANUV) AS TICK_ANUV,"
'strSql1 = strSql1 & vbCrLf & "     SUM(C.VALOR) AS BMFALSOS, TOTALDESC, TOTALDISC"
'strSql1 = strSql1 & vbCrLf & "     FROM PEA51T00_2 A, BMFALSOS C,"
'strSql1 = strSql1 & vbCrLf & "     (SELECT CODIGO_CAJERO, CODIGO_ESTACION, TO_NUMBER(SUM(IMPORTE)) AS TOTALDESC FROM DESCUENTOS_2 A,"
'strSql1 = strSql1 & vbCrLf & "     TIPODESC B WHERE A.CODIGO_CAJERO < '9500' AND"
'strSql1 = strSql1 & vbCrLf & "     a.FECHA_ULTIMO_PERIODO Between  '" & fDesde & "' AND '" & fHasta & "'"
'strSql1 = strSql1 & vbCrLf & "     AND B.LIQUIDACION=1 AND IMPORTEDEF IS NULL"
'strSql1 = strSql1 & vbCrLf & "     AND A.CODMOTIVO=B.CODIGO"
'strSql1 = strSql1 & vbCrLf & "     GROUP BY CODIGO_CAJERO, CODIGO_ESTACION"
'strSql1 = strSql1 & vbCrLf & "     ORDER BY 1 ) D,"
'strSql1 = strSql1 & vbCrLf & "     (SELECT CODIGO_CAJERO,CODIGO_ESTACION, TO_NUMBER(SUM(IMPORTE)) AS TOTALDISC FROM DESCUENTOS_2 A,"
'strSql1 = strSql1 & vbCrLf & "     TIPODESC B WHERE A.CODIGO_CAJERO < '9500' AND"
'strSql1 = strSql1 & vbCrLf & "     a.FECHA_ULTIMO_PERIODO Between  '" & fDesde & "' AND '" & fHasta & "'"
'strSql1 = strSql1 & vbCrLf & "     AND B.LIQUIDACION<>1"
'strSql1 = strSql1 & vbCrLf & "     AND A.CODMOTIVO=B.CODIGO"
'strSql1 = strSql1 & vbCrLf & "     GROUP BY CODIGO_CAJERO ,CODIGO_ESTACION"
'strSql1 = strSql1 & vbCrLf & "     ORDER BY 1 ) E,"
'strSql1 = strSql1 & vbCrLf & "     (SELECT CODIGO_CAJERO, CODIGO_ESTACION, SUM(TICK_ANUV) AS TICK_ANUV,SUM(TICK_ANUL) AS TICK_ANUL "
'strSql1 = strSql1 & vbCrLf & "     FROM PEA51T00_2 A WHERE CODIGO_CAJERO < '9500' AND FECHA_ULTIMO_PERIODO "
'strSql1 = strSql1 & vbCrLf & "     Between '" & fDesde & "' AND '" & fHasta & "' "
'strSql1 = strSql1 & vbCrLf & "     GROUP BY CODIGO_CAJERO, CODIGO_ESTACION) F,"
'strSql1 = strSql1 & vbCrLf & "     PEA12T00 h"
'strSql1 = strSql1 & vbCrLf & "  Where "
'strSql1 = strSql1 & vbCrLf & "     A.FECHA_ULTIMO_PERIODO = C.FECHA_ULTIMO_PERIODO(+) AND "
'strSql1 = strSql1 & vbCrLf & "     A.CODIGO_CAJERO = C.CODIGO_CAJERO(+) AND "
'strSql1 = strSql1 & vbCrLf & "     A.CODIGO_ESTACION = C.CODIGO_ESTACION(+) AND "
'strSql1 = strSql1 & vbCrLf & "     A.HORA_ULTIMO_PERIODO = C.HORA_ULTIMO_PERIODO(+) AND "
'strSql1 = strSql1 & vbCrLf & "     A.CODIGO_CAJERO = D.CODIGO_CAJERO(+) AND "
